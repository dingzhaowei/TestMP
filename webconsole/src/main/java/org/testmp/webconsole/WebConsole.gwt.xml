<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
    "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
    
<module rename-to='webconsole'>

      <!-- Inherit the core GWT stuff. -->
      <inherits name='com.google.gwt.user.User'/>
      <inherits name="com.google.gwt.i18n.I18N"/>
      <inherits name="com.google.gwt.json.JSON"/>
      <inherits name="com.google.gwt.http.HTTP"/>

      <!-- Inherit the SmartGWT stuff. -->
      <inherits name='com.smartgwt.SmartGwt' />
      <inherits name="com.smartgwt.tools.SmartGwtTools"/>

      <!-- <inherits name="com.smartclient.theme.graphite.Graphite"/> -->
      <inherits name="com.smartclient.theme.enterprise.Enterprise"/>

      <!-- Limit builds to your preferred browser. -->

      <!-- Safari -->
      <!-- <set-property name="user.agent" value="safari"/> -->

      <!-- Firefox 3 -->
      <!-- <set-property name="user.agent" value="gecko1_8"/> -->

      <!-- Specify the app entry point class. -->
      <entry-point class='org.testmp.webconsole.client.WebConsole'/>

      <!-- Specify the application-specific style sheet. -->
      <stylesheet src='WebConsole.css' />

      <!-- <extend-property name="locale" values="en_US,zh_CN"/> -->
      <!-- <set-property-fallback name="locale" value="en" /> -->
      <extend-property name="locale" values="zh"/>

      <source path='client' />
      <source path='shared' />
</module>
